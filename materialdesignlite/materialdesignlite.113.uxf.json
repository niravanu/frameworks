{
    "id": "material-design-lite113",
    "name": "Material Design Lite 1",
    "site": "https://getmdl.io/",
    "version": "1.1.3",
    "styles": [
        "https://code.getmdl.io/1.1.3/material.indigo-pink.min.css"
    ],
    "scripts": [
        "https://code.getmdl.io/1.1.3/material.min.js"
    ],
    "themes": [{
        "name": "Indigo - Pink",
        "url": "https://code.getmdl.io/1.1.3/material.indigo-pink.min.css"
    }, {
        "name": "Indigo - Red",
        "url": "https://code.getmdl.io/1.1.3/material.indigo-red.min.css"
    }, {
        "name": "Orange - Teal",
        "url": "https://code.getmdl.io/1.1.3/material.deep_orange-teal.min.css"
    }, {
        "name": "Green - Deep Purple",
        "url": "https://code.getmdl.io/1.1.3/material.light_green-deep_purple.min.css"
    }],
    "tags": [{
        "id": "header",
        "name": "Header",
        "template": "<h1 if=\"{opts.type=='h1'}\">{opts.text}</h1><h2 if=\"{opts.type=='h2'}\">{opts.text}</h2><h3 if=\"{opts.type == 'h3'}\">{opts.text}</h3><h4 if=\"{opts.type == 'h4'}\">{opts.text}</h4>",
        "options": [{
            "id": "type",
            "name": "Type",
            "tag": "select",
            "options": ["h1", "h2", "h3", "h4"],
            "value": "h2"
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Header"
        }]
    }, {
        "id": "button",
        "name": "Button",
        "template": "<button class='mdl-button mdl-js-button {opts.effect} {opts.color}' disabled=\"{opts.disabled}\" type='button'> {opts.label} </button>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Button"
        }, {
            "id": "effect",
            "name": "Effect",
            "tag": "select",
            "options": ["", "mdl-button--raised", "mdl-button--fab", "mdl-button--mini-fab", "mdl-button--icon"],
            "value": "mdl-button--raised"
        }, {
            "id": "color",
            "name": "Color",
            "tag": "select",
            "options": ["", "mdl-button--colored", "mdl-button--primary", "mdl-button--accent"],
            "value": "mdl-button--colored"
        }, {
            "id": "disabled",
            "name": "Disabled",
            "tag": "checkbox",
            "value": false
        }]
    }]
}
