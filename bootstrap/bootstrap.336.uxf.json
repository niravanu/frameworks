{
    "id": "bootstrap336",
    "name": "Bootstrap 3",
    "site": "http://getbootstrap.com/",
    "version": "3.3.6",
    "url": "",
    "styles": [
        "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css"
    ],
    "scripts": [
        "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js"
    ],
    "themes": [{
        "name": "cerulean",
        "url": "https://rawgit.com/thomaspark/bootswatch/cerulean/bootstrap.min.css"
    }, {
        "name": "Lumen",
        "url": "https://rawgit.com/thomaspark/bootswatch/lumen/bootstrap.min.css"
    }],
    "tags": [{
        "id": "header",
        "name": "Header",
        "template": "<h1 if=\"{opts.type=='h1'}\">{opts.text}</h1><h2 if=\"{opts.type=='h2'}\">{opts.text}</h2><h3 if=\"{opts.type == 'h3'}\">{opts.text}</h3><h4 if=\"{opts.type == 'h4'}\">{opts.text}</h4>",
        "options": [{
            "id": "type",
            "name": "Type",
            "tag": "select",
            "options": ["h1", "h2", "h3", "h4"],
            "value": "h2"
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Header"
        }]
    }, {
        "id": "table",
        "name": "Table",
        "template": "<table class='table {opts.striped} {opts.bordered} {opts.hover} {opts.condensed}'><thead><tr><th each={[opts.cols]}>H</th></tr></thead><tbody><tr each={[opts.rows]}><td each={[opts.cols]}>C</td></tr></tbody></table>",
        "options": [{
            "id": "striped",
            "name": "Striped",
            "tag": "checkbox",
            "options": ["", "table-striped"],
            "value": ""
        }, {
            "id": "bordered",
            "name": "Bordered",
            "tag": "checkbox",
            "options": ["", "table-bordered"],
            "value": ""
        }, {
            "id": "hover",
            "name": "Hover",
            "tag": "checkbox",
            "options": ["", "table-hover"],
            "value": ""
        }, {
            "id": "condensed",
            "name": "Condensed",
            "tag": "checkbox",
            "options": ["", "table-condensed"],
            "value": ""
        }, {
            "id": "cols",
            "name": "Cols",
            "tag": "input",
            "value": 3
        }, {
            "id": "rows",
            "name": "Rows",
            "tag": "input",
            "value": 3
        }]
    }, {
        "id": "button",
        "name": "Button",
        "template": "<button class='btn {opts.style} {opts.size} {opts.block} {opts.disabled}' type='button'> {opts.label} </button>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Button"
        }, {
            "id": "size",
            "name": "Size",
            "tag": "select",
            "options": ["btn-lg", "btn-def", "btn-sm", "btn-xs"],
            "value": "btn-def"
        }, {
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["btn-default", "btn-primary", "btn-success", "btn-info", "btn-warning", "btn-danger", "btn-link"],
            "value": "btn-default"
        }, {
            "id": "block",
            "name": "Block",
            "tag": "checkbox",
            "options": ["", "btn-block"],
            "value": ""
        }, {
            "id": "disabled",
            "name": "Disabled",
            "tag": "checkbox",
            "options": ["", "disabled"],
            "value": ""
        }]
    }, {
        "id": "image",
        "name": "Image",
        "template": "<img draggable='false' src='http://lorempixel.com/g/150/150' alt='image' class = '{opts.style}'/>",
        "options": [{
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["", "img-rounded", "img-circle", "img-thumbnail"],
            "value": ""
        }]
    }, {
        "id": "paragraph",
        "name": "Paragraph",
        "template": "<p class='{opts.style} {opts.background}'>{opts.text}</p>",
        "options": [{
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["", "text-muted", "text-primary", "text-success", "text-info", "text-warning", "text-danger"],
            "value": ""
        }, {
            "id": "background",
            "name": "Background",
            "tag": "select",
            "options": ["", "bg-primary", "bg-success", "bg-info", "bg-warning", "bg-danger"],
            "value": ""
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Fusce dapibus, tellus ac cursus commodo, tortor mauris nibh."
        }]
    }, {
        "id": "label",
        "name": "Label",
        "template": "<span class='label {opts.style}'> {opts.label} </span>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Label"
        }, {
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["label-default", "label-primary", "label-success", "label-info", "label-warning", "label-danger"],
            "value": "label-default"
        }]
    }, {
        "id": "badge",
        "name": "Badge",
        "template": "<span class='badge'> {opts.label} </span>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "10"
        }]
        }, {
            "id": "jumbotron",
            "name": "Jumbotron",
            "template": "<div class='jumbotron'><h1>Hello, world!</h1><p>...</p><p><a class='btn btn-primary btn-lg' href='#' role='button'>Learn more</a></p></div>",
            "options": []
        }
  ]
}
