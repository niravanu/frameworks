{
    "id": "bootstrap336",
    "name": "Bootstrap 3",
    "site": "http://getbootstrap.com/",
    "version": "3.3.6",
    "url": "",
    "styles": [
        "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css"
    ],
    "scripts": [
        "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js"
    ],
    "themes": [{
        "name": "cerulean",
        "url": "https://rawgit.com/thomaspark/bootswatch/cerulean/bootstrap.min.css"
    }, {
        "name": "Lumen",
        "url": "https://rawgit.com/thomaspark/bootswatch/lumen/bootstrap.min.css"
    }],
    "tags": [{
        "id": "header",
        "name": "Header",
        "template": "<h1 if=\"{opts.type=='h1'}\">{opts.text}</h1><h2 if=\"{opts.type=='h2'}\">{opts.text}</h2><h3 if=\"{opts.type == 'h3'}\">{opts.text}</h3><h4 if=\"{opts.type == 'h4'}\">{opts.text}</h4>",
        "options": [{
            "id": "type",
            "name": "Type",
            "tag": "select",
            "options": ["h1", "h2", "h3", "h4"],
            "value": "h2"
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Header"
        }]
        }, {
            "id": "table",
            "name": "Table",
            "template": "<table class='table {opts.style} {opts.size} {opts.block}' disabled=\"{opts.disabled}\" type='button'> {opts.label} </table>",
            "options": [{
                "id": "style",
                "name": "Style",
                "tag": "select",
                "options": ["", "table-striped", "table-bordered", "btn-info", "btn-warning", "btn-danger", "btn-link"],
                "value": "btn-default"
            }]
    }, {
        "id": "button",
        "name": "Button",
        "template": "<button class='btn {opts.style} {opts.size} {opts.block} {opts.disabled}' type='button'> {opts.label} </button>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Button"
        }, {
            "id": "size",
            "name": "Size",
            "tag": "select",
            "options": ["btn-lg", "btn-def", "btn-sm", "btn-xs"],
            "value": "btn-def"
        }, {
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["btn-default", "btn-primary", "btn-success", "btn-info", "btn-warning", "btn-danger", "btn-link"],
            "value": "btn-default"
        }, {
            "id": "block",
            "name": "Block",
            "tag": "checkbox",
            "options": ["", "btn-block"],
            "value": ""
        }, {
            "id": "disabled",
            "name": "Disabled",
            "tag": "checkbox",
            "options": ["", "disabled"],
            "value": ""
        }]
    }, {
        "id": "label",
        "name": "Label",
        "template": "<span class='label {opts.style}'> {opts.label} </span>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Label"
        }, {
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["label-default", "label-primary", "label-success", "label-info", "label-warning", "label-danger"],
            "value": "label-default"
        }]
    }]
}
