{
    "id": "bootstrap336",
    "name": "Bootstrap 3",
    "site": "http://getbootstrap.com/",
    "version": "3.3.6",
    "url": "",
    "styles": [
        "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css"
    ],
    "scripts": [
        "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js"
    ],
    "themes": [{
        "name": "default",
        "url": "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css"
    }, {
        "name": "cerulean",
        "url": "https://rawgit.com/thomaspark/bootswatch/cerulean/bootstrap.min.css"
    }, {
        "name": "Lumen",
        "url": "https://rawgit.com/thomaspark/bootswatch/lumen/bootstrap.min.css"
    }],
    "tags": [{
        "id": "header",
        "name": "Header",
        "template": "<h1 if=\"{opts.type=='h1'}\">{opts.text}</h1><h2 if=\"{opts.type=='h2'}\">{opts.text}</h2><h3 if=\"{opts.type == 'h3'}\">{opts.text}</h3><h4 if=\"{opts.type == 'h4'}\">{opts.text}</h4>",
        "options": [{
            "id": "type",
            "name": "Type",
            "tag": "select",
            "options": ["h1", "h2", "h3", "h4"],
            "value": "h2"
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Header"
        }]
    }, {
        "id": "table",
        "name": "Table",
        "template": "<table class='table {opts.striped} {opts.bordered} {opts.hover} {opts.condensed}'><thead><tr><th each={[opts.cols]}>H</th></tr></thead><tbody><tr each={[opts.rows]}><td each={[opts.cols]}>C</td></tr></tbody></table>",
        "options": [{
            "id": "striped",
            "name": "Striped",
            "tag": "checkbox",
            "options": ["", "table-striped"],
            "value": ""
        }, {
            "id": "bordered",
            "name": "Bordered",
            "tag": "checkbox",
            "options": ["", "table-bordered"],
            "value": ""
        }, {
            "id": "hover",
            "name": "Hover",
            "tag": "checkbox",
            "options": ["", "table-hover"],
            "value": ""
        }, {
            "id": "condensed",
            "name": "Condensed",
            "tag": "checkbox",
            "options": ["", "table-condensed"],
            "value": ""
        }, {
            "id": "cols",
            "name": "Cols",
            "tag": "input",
            "value": 3
        }, {
            "id": "rows",
            "name": "Rows",
            "tag": "input",
            "value": 3
        }]
    }, {
        "id": "button",
        "name": "Button",
        "template": "<button class='btn {opts.style} {opts.size} {opts.block} {opts.disabled}' type='button'> {opts.label} </button>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Button"
        }, {
            "id": "size",
            "name": "Size",
            "tag": "select",
            "options": ["btn-lg", "btn-def", "btn-sm", "btn-xs"],
            "value": "btn-def"
        }, {
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["btn-default", "btn-primary", "btn-success", "btn-info", "btn-warning", "btn-danger", "btn-link"],
            "value": "btn-default"
        }, {
            "id": "block",
            "name": "Block",
            "tag": "checkbox",
            "options": ["", "btn-block"],
            "value": ""
        }, {
            "id": "disabled",
            "name": "Disabled",
            "tag": "checkbox",
            "options": ["", "disabled"],
            "value": ""
        }]
    }, {
        "id": "image",
        "name": "Image",
        "template": "<img draggable='false' src={opts.src} alt={opts.alt} height={opts.height} width={opts.width} class ='{opts.style}'/>",
        "options": [{
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["", "img-rounded", "img-circle", "img-thumbnail"],
            "value": ""
        }, {
            "id": "src",
            "name": "Src",
            "tag": "input",
            "value": "http://lorempixel.com/g/150/150"
        }, {
            "id": "alt",
            "name": "Alt",
            "tag": "input",
            "value": "image"
        }, {
            "id": "width",
            "name": "Width",
            "tag": "input",
            "value": "150"
        }, {
            "id": "height",
            "name": "Height",
            "tag": "input",
            "value": "150"
        }]
    }, {
        "id": "paragraph",
        "name": "Paragraph",
        "template": "<p class='{opts.style} {opts.background}'>{opts.text}</p>",
        "options": [{
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["", "text-muted", "text-primary", "text-success", "text-info", "text-warning", "text-danger"],
            "value": ""
        }, {
            "id": "background",
            "name": "Background",
            "tag": "select",
            "options": ["", "bg-primary", "bg-success", "bg-info", "bg-warning", "bg-danger"],
            "value": ""
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Fusce dapibus, tellus ac cursus commodo, tortor mauris nibh."
        }]
    }, {
        "id": "breadcrumbs",
        "name": "Breadcrumbs",
        "template": "<ol class='breadcrumb'><li><a href='#'>Home</a></li><li><a href='#'>Library</a></li><li class='active'>Data</li></ol>",
        "options": []
    }, {
        "id": "pagination",
        "name": "Pagination",
        "template": "<nav aria-label='Page navigation'><ul class='pagination {opts.size}'><li><a href='#' aria-label='Previous'><span aria-hidden='true'>&laquo;</span></a></li><li><a href='#'>1</a></li><li><a href='#'>2</a></li><li><a href='#'>3</a></li><li><a href='#' aria-label='Next'><span aria-hidden='true'>&raquo;</span></a></li></ul></nav>",
        "options": [{
            "id": "size",
            "name": "Size",
            "tag": "select",
            "options": ["", "pagination-sm", "pagination-lg"],
            "value": ""
        }]
    }, {
        "id": "pager",
        "name": "Pager",
        "template": "<nav aria-label='Pager'><ul class='pager'><li class={opts.align=='align'?'previous':''}><a href='#'>Previous</a></li><li class={opts.align=='align'?'next':''}><a href='#'>Next</a></li></ul></nav>",
        "options": [{
            "id": "align",
            "name": "Align",
            "tag": "checkbox",
            "options": ["", "align"],
            "value": ""
        }]
    }, {
        "id": "label",
        "name": "Label",
        "template": "<span class='label {opts.style}'> {opts.label} </span>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Label"
        }, {
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["label-default", "label-primary", "label-success", "label-info", "label-warning", "label-danger"],
            "value": "label-default"
        }]
    }, {
        "id": "badge",
        "name": "Badge",
        "template": "<span class='badge'> {opts.label} </span>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "10"
        }]
    }, {
        "id": "jumbotron",
        "name": "Jumbotron",
        "template": "<div class='jumbotron'><h1>Hello, world!</h1><p>...</p><p><a class='btn btn-primary btn-lg' href='#' role='button'>Learn more</a></p></div>",
        "options": []
    }, {
        "id": "pageheader",
        "name": "Page Header",
        "template": "<div class='page-header'><h1>Example page header <small>Subtext for header</small></h1></div>",
        "options": []
    }, {
        "id": "thumbnails",
        "name": "Thumbnails",
        "template": "<div class='row'><div class='col-xs-6 col-md-3'><div class='thumbnail'><img src='http://lorempixel.com/g/150/150' alt='image'><div class='caption'>Caption</div></div></div><div class='col-xs-6 col-md-3'><div class='thumbnail'><img src='http://lorempixel.com/g/150/150' alt='image'><div class='caption'>Caption</div></div></div></div>",
        "options": []
    }, {
        "id": "alert",
        "name": "Alert",
        "template": "<div class='alert {opts.style} {opts.dismissable}' role='alert'><button if={opts.dismissable=='alert-dismissible'} type='button' class='close' data-dismiss='alert' aria-label='Close'><span aria-hidden='true'>&times;</span></button>{opts.text}</div>",
        "options": [{
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["", "alert-success", "alert-info", "alert-warning", "alert-danger"],
            "value": ""
        }, {
            "id": "dismissable",
            "name": "Dismissable",
            "tag": "checkbox",
            "options": ["", "alert-dismissible"],
            "value": ""
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Fusce dapibus, tellus ac cursus commodo, tortor mauris nibh."
        }]
    }, {
        "id": "listgroup",
        "name": "List Group",
        "template": "<ul class='list-group'><li class='list-group-item'>One</li><li class='list-group-item'>Two</li><li class='list-group-item'>Three</li></ul>",
        "options": []
    }, {
        "id": "panel",
        "name": "Panel",
        "template": "<div class='panel {opts.style}'><div if={opts.heading=='true'} class='panel-heading'><h3 class='panel-title'>Title</h3></div><div class='panel-body'>{opts.text}</div><div if={opts.footer=='true'} class='panel-footer'>Panel footer</div></div>",
        "options": [{
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["panel-default", "panel-primary", "panel-success", "panel-info", "panel-warning", "panel-danger"],
            "value": "panel-default"
        }, {
            "id": "heading",
            "name": "Heading",
            "tag": "checkbox",
            "options": ["", "true"],
            "value": ""
        }, {
            "id": "footer",
            "name": "Footer",
            "tag": "checkbox",
            "options": ["", "true"],
            "value": ""
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Fusce dapibus, tellus ac cursus commodo, tortor mauris nibh."
        }]
    }]
}
