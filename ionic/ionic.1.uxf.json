{
    "id": "ionic.1",
    "name": "Ionic",
    "site":"http://ionicframework.com/",
    "version": "1.3.1",
    "styles": [
        "https://cdnjs.cloudflare.com/ajax/libs/ionic/1.3.1/css/ionic.min.css"
    ],
    "scripts": [
        "https://cdnjs.cloudflare.com/ajax/libs/ionic/1.3.1/js/ionic.min.js"
    ],
    "tags": [{
        "id": "header",
        "name": "Header",
        "template": "<h1 if=\"{opts.type=='h1'}\">{opts.text}</h1><h2 if=\"{opts.type=='h2'}\">{opts.text}</h2><h3 if=\"{opts.type == 'h3'}\">{opts.text}</h3><h4 if=\"{opts.type == 'h4'}\">{opts.text}</h4>",
        "options": [{
            "id": "type",
            "name": "Type",
            "tag": "select",
            "options": ["h1", "h2", "h3", "h4"],
            "value": "h2"
        }, {
            "id": "text",
            "name": "Text",
            "tag": "input",
            "value": "Header"
        }]
    }, {
        "id": "button",
        "name": "Button",
        "template": "<button class='button {opts.style} {opts.size}' disabled=\"{opts.disabled}\" type='button'> {opts.label} </button>",
        "options": [{
            "id": "label",
            "name": "Label",
            "tag": "input",
            "value": "Button"
        }, {
            "id": "size",
            "name": "Size",
            "tag": "select",
            "options": ["button-large", "button-default", "button-small"],
            "value": "button-default"
        }, {
            "id": "style",
            "name": "Style",
            "tag": "select",
            "options": ["button-default", "button-light", "button-stable", "button-positive", "button-calm", "button-balanced", "button-energized", "button-assertive", "button-royal", "button-dark"],
            "value": "button-default"
        }, {
            "id": "disabled",
            "name": "Disabled",
            "tag": "checkbox",
            "value": false
        }]
    }]
}
